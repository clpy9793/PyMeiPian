    new Vue({
        el: '#video0',
        components: {
            'vVideo': window.vueVideo
        },
        methods: {
            videoPlaying: function () {
                var music = document.getElementById("bgsound");
                if (music !== null && ! music.paused) {
                    var icon = document.getElementById("music_icon");
                    icon.setAttribute("play","on");
                    music.pause();
                }
            }
        },
        beforeCreate: function () {
            console.log(this)
        },
        created: function () {
            console.log(this)
        }
    });
æ¯å­è¶…é…·ï½ž  å–å®Œè¿˜å¯ä»¥æ‹¿å›žå®¶åšçºªå¿µðŸ˜é¥­åŽå†é€›é€›ï¼Œç»“æŸäº†ç¬¬ä¸€å¤©çš„å¡žç­è¡Œç¨‹ã€‚æƒ³æƒ³æ˜Žå¤©å†›èˆ°å²›ä¸€æ—¥æ¸¸è¦8ç‚¹é›†åˆï¼Œä¹Ÿæƒ³å¿«å¿«ç¡è§‰äº†ï¼ˆå¯å¤ªæ—©ç¡ä¸ç€å•ŠðŸ˜¥ï¼‰å°tipsï¼šå¡žç­çš„æ°´ä¸å®œç›´æŽ¥é¥®ç”¨ï¼Œæˆ‘ä»¬æ¯ä¸¤å¤©å°±è¦ä¹°ä¸€ç“¶ä¸€åŠ ä»‘çš„æ°´ï¼ˆ4åˆ€ï¼‰ã€‚ç”±äºŽé¥®ç”¨æ°´çš„çŸœè´µï¼Œæ‰€ä»¥åœ¨éº¦å½“åŠ³å’Œæœºåœºè¿™äº›åœ°æ–¹ï¼Œæ˜¯æ²¡æœ‰å…è´¹æ°´æä¾›çš„ã€‚å›žç¨‹çš„æ—¶å€™æˆ‘ä»¬è¿˜ä»¥ä¸ºå¡žç­æœºåœºå’Œå…¶ä»–åœ°æ–¹ä¸€æ ·ä¼šæä¾›å…è´¹çƒ­æ°´ï¼Œæ‹¿ç€çƒ­æ°´å£¶åŽ»é—®ï¼Œä»–ä»¬å°±è¯´ï¼šâ€œ3 dollarsâ€ðŸ˜±å¤©å•Šï½žï½ž ä¸€å°ç“¶çƒ­æ°´è¦3åˆ€ï¼ç¬¬ä¸‰å¤©ç¬¬ä¸‰å¤©ä»Šå¤©è®¢äº†å†›èˆ°å²›ä¸€æ—¥æ¸¸ï¼Œå…«ç‚¹åäº”åˆ†å°±æœ‰è½¦æ¥æŽ¥æˆ‘ä»¬äº†ï¼ˆå¥½å›°å•Šâ€¦â€¦Bensonè¿˜æ²¡ç¡é†’ï¼Œå¹¸å¥½ä¸ƒç‚¹å¤šå«é†’ä»–ï¼Œä»–ä¹Ÿæ²¡æœ‰å“­é—¹ï¼Œå°±æ˜¯å‘†å‘†çš„ç›´åˆ°ç™»èˆ¹ï¼‰ã€‚åŽŸæ¥æˆ‘ä»¬é…’åº—åŽé¢å°±æ˜¯ä¸€ä¸ªå¿«è‰‡ðŸš¤ç å¤´ï¼Œè‡ªå¤‡äº†å©´å„¿æ•‘ç”Ÿè¡£ï¼ŒBensonå¾ˆå¥½å¥‡åœ°è·Ÿæˆ‘ä»¬ç™»èˆ¹äº†ï¼ˆç”±äºŽçŸ¥é“åˆ°å†›èˆ°å²›èˆ¹ç¨‹åªæœ‰ååˆ†é’Ÿï¼Œæ‰€ä»¥å°±å¸¦ä¸ŠBensonå°è¯•ä¸€ä¸‹ã€‚ä¹‹å‰ä¸€å²çš„æ—¶å€™åœ¨æ™®å‰åŽ»çš‡å¸å²›ï¼ŒåŠä¸ªå°æ—¶çš„èˆ¹ç¨‹ï¼Œæˆ‘è¿˜æ˜¯å®æ„¿è´µä¸€ç‚¹ï¼Œåäº†æœ‰é¡¶æœ‰çª—çš„å¿«èˆ¹å¦¥å½“ä¸€äº›ðŸ˜³ï¼‰å“‡â€¦â€¦è¿™ç‰‡å¤§æµ·çš„è“ç¡®å®žå¤ªè¿·äººäº†ï¼æ„Ÿè§‰è“å‡ºäº†ä¸€ä¸ªæ–°é«˜åº¦ï¼Œä»¥å‰ä»Žæœªè§è¿‡ï¼Bensonåˆç´§å¼ åœ°æŠ“ä½æˆ‘çš„æ‰‹ï¼Œåˆå…´å¥‹å¿«è‰‡çš„é€Ÿåº¦ä¸Žæ¿€æƒ…ðŸ˜œï¼ˆå›žç¨‹çš„æ—¶å€™ï¼Œè¿™å­©å­å±…ç„¶èƒ½åœ¨å¿«è‰‡ä¸Šç¡ç€äº†â€¦å“ˆå“ˆå“ˆï¼‰æ¤°æž—æ ‘å½±ðŸŒ´ðŸŒ´ðŸŒ´æ¸…å¿ƒè“çš„æµ·æ°´ðŸ˜šå“ˆï½žåœ¨æœºåœºè¿ªå£«å°¼ä¹°çš„å¸½å­æŒºæŠ¢é•œçš„ðŸ˜œå†›èˆ°å²›çš„è“å¤ªçº¯ç²¹è€Œåˆé€å¿ƒäº†ðŸ’—å¡žç­ä¸ƒè‰²æµ·çš„å±‚æ¬¡ðŸŒŠðŸŒŠðŸŒŠæ¸…æ¾ˆè§åº•ðŸ‘€è“ï½žï½žï½žï½žï½žå¤ªç¾Žçš„åœ°æ–¹å¿ä¸ä½å¤šè‡ªæ‹äº†å‡ å¼ ðŸ˜ðŸ˜œæ‹‰ç€å„¿å­åŽ»è¸©æ²™ðŸ˜Šæµ®æ½œçš„ä¸–ç•Œï½žè¿½ç€é±¼å„¿è·‘ï¼æ„Ÿè§‰å¤ªè‡ªç”±è‡ªåœ¨äº†ðŸ ðŸŸðŸ ðŸŸåœ¨å†›èˆ°å²›åœ°æ ‡ç•™å½±ï½žåˆ°æ­¤ä¸€æ¸¸ðŸ˜çŽ©å¾—å¾ˆçˆ½çš„å°å®è´ðŸ˜Šæˆ‘ä»¬ä»Žæ—©ä¸Šä¹ç‚¹å¤šçŽ©åˆ°ä¸‹åˆä¸¤ç‚¹åŠï¼Œæ­£å½“çƒˆæ—¥å½“ç©ºçš„æ—¶å€™å°±å›žåŽ»ä¼‘æ¯äº†ï¼å¡žç­çš„ä¸­åˆå¤ªé˜³å¾ˆæ¯’è¾£â€¦â€¦åå‰¯å…¶å®žçš„â€œæ™’ç­â€â˜€ðŸ˜‚å†›èˆ°å²›çš„ç å¤´å›žé…’åº—å„è‡ªæ¢³æ´—å®Œæ¯•ï¼Œæˆ‘ä»¬ä»¨éƒ½ç¡äº†ä¸€ä¸ªæ¼«é•¿çš„åˆè§‰ï¼Œå¤ªçˆ½äº†ï½žï½žç»ˆäºŽåƒä